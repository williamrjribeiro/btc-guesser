{
  "name": "btc-guesser",
  "version": "0.0.1",
  "description": "An online game for guessing the prices of crypto currencies",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc -b && vite build",
    "ci:format": "prettier --check .",
    "ci:lint": "eslint .",
    "ci:typecheck": "tsc --noEmit",
    "ci:test": "vitest run",
    "dev": "vite",
    "format": "prettier --write .",
    "lint": "eslint . --fix",
    "preview": "vite preview",
    "test": "vitest",
    "sst:deploy": "sst deploy",
    "sst:remove": "sst remove",
    "aws:sso": "aws sso login --sso-session=fire-emblem"
  },
  "dependencies": {
    "@preact/signals": "^2.0.4",
    "pollinator": "^0.3.2",
    "preact": "^10.26.5",
    "sst": "^3.14.28",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@eslint/json": "^0.12.0",
    "@preact/preset-vite": "^2.10.1",
    "@tsconfig/node22": "^22",
    "@types/aws-lambda": "8.10.149",
    "@types/node": "22",
    "@types/uuid": "^10.0.0",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "globals": "^16.1.0",
    "prettier": "^3.5.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.32.1",
    "vite": "^6.3.5",
    "vitest": "^3.1.4"
  },
  "author": "William R. J. Ribeiro",
  "license": "Unlicense",
  "packageManager": "pnpm@10.7.1+sha512.2d92c86b7928dc8284f53494fb4201f983da65f0fb4f0d40baafa5cf628fa31dae3e5968f12466f17df7e97310e30f343a648baea1b9b350685dafafffdf5808",
  "engines": {
    "node": ">=22",
    "pnpm": ">=10"
  }
}
